<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="5dip" >
    <TextView
        android:id="@+id/nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ff0000"
        android:textStyle="bold"/>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal" >

    <TextView
        android:id="@+id/label0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:text="#" />
    <TextView
        android:id="@+id/id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:textColor="#111234" />

    <TextView
        android:id="@+id/label1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:text="E" />
    <TextView
        android:id="@+id/empresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:textColor="#111234" />


    <TextView
        android:id="@+id/label2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:text="C" />
    <TextView
        android:id="@+id/cuartel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:textColor="#340f19" />

        <TextView
            android:id="@+id/label20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontStyle="normal"
            android:text="T" />
        <TextView
            android:id="@+id/telefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontStyle="normal"
            android:textColor="#340f19" />

        <TextView
            android:id="@+id/label201"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontStyle="normal"
            android:text="S" />
        <TextView
            android:id="@+id/nosinc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontStyle="normal"
            android:textColor="#340f19" />

        <TextView
        android:id="@+id/label3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:text="PRO" />
    <TextView
        android:id="@+id/producto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:textColor="#343434"/>

    <TextView
        android:id="@+id/label4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:text="VAR" />
    <TextView
        android:id="@+id/variedad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:textColor="#303411" />

    <TextView
        android:id="@+id/label5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:text="Q=" />
    <TextView
        android:id="@+id/cantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontStyle="normal"
        android:textColor="#093433" />

    </LinearLayout>

</LinearLayout>
